@charset "utf-8";

@import url("../webfonts/gost_NEW/stylesheet.css");

/* CSS Document */

/*******************************
 =============HTML5=============
 *******************************/
 
section, header, footer, aside, article, nav {
	display:block;	
}

body, html {
	background-color: #F8F8F3;
	font-family: "gost NEW";
	height: 100%;
	margin: 0;
	padding: 0;
}

* html #wrapper {
    height: 100%;
}
.page-buffer {
	height: 80px;
}

a {
	text-decoration: none;
	color:black;	
}

a:hover {
	text-decoration: none;
	color:white;
}

p {
	font-size: 14px;
	line-height: 18px;
	font-family: "gost NEW";
	text-align: left;
}

h1,h2,h3,h4,h5,h6 {
	font-family: "gost NEW";
	text-transform: uppercase;
	font-weight:normal;
}

#wrapper {
	display: block;
	margin-right: auto;
	margin-left: auto;
	height: auto;
	min-height: 100%;
	margin-bottom: -80px;
	position: relative;
}

h3 {
	font-size: 15px;
	text-align:center;
}

#logo {
	padding-top: 20px;
	text-align:center;
}

ul {
	overflow:hidden;
}

li {
	list-style:none;
}

.container_12 {
	position:relative;
}

/*******************************
 ==========MENU STYLES==========
 *******************************/

nav {
	overflow: hidden;
	float: left;
	margin-top: 30px;
	margin-bottom: 20px;
	height: 35px;
	line-height: 35px;
	margin-left: 35px;
	z-index: 800;
	clear: both;
	position: relative;
}

nav li {
	float: left;
	margin-left: 0px;
	margin-right: 2px;
	text-transform: uppercase;
}

	#navcenter {
		margin-left:auto;
		margin-right: auto;
		width: 630px;
	}
	
	nav ul li a {
	font-size: 13px;
	font-weight: normal;
	padding-top: 8px;
	padding-right: 8px;
	padding-bottom: 8px;
	padding-left: 8px;
	}
	
	nav ul li a:hover, nav ul li .selected {
	background:#80C6C6;
	border-bottom:none;
	text-decoration:none;
	-webkit-transition:background 0.3s;
	-moz-transition: background 0.3s;
	-o-transition:background 0.3s;
	transition:background 0.3s;
	}
	
