@charset "utf-8";

@import url("../webfonts/gost_NEW/stylesheet.css");

/* CSS Document */

/*******************************
 =============HTML5=============
 *******************************/
 
section, header, footer, aside, article, nav {
	display:block;	
}

body, html {
	background-color: #F8F8F3;
	font-family: "gost NEW";
	height: 100%;
	margin: 0;
	padding: 0;
}

* html #wrapper {
    height: 100%;
}
.page-buffer {
	height: 80px;
}

a {
	text-decoration: none;
	color:black;	
}

a:hover {
	text-decoration: none;
	color:white;
}

p {
	font-size: 12px;
	line-height: 18px;
	font-family: "gost NEW";
}

h1,h2,h3,h4,h5,h6 {
	font-family: "gost NEW";
	text-transform: uppercase;
	font-weight:normal;
}